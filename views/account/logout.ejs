<h1><%- title %></h1>

<p><%- messages() %></p> 

<% if (errors) { %>
    <ul class="notice">
   <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
  <% } %>


  <form class="form" id="logout-form" action="/account/logout" method="post">
    <button id="logout-button" type="submit">LOGOUT</button>
  </form>

